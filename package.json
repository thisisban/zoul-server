{
  "name": "zoul-server",
  "version": "1.0.0",
  "description": "Backend for audio platform",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "dev:network": "NODE_ENV=development nodemon index.js",
    "start": "node index.js",
    "build": "prisma generate && prisma migrate deploy",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest --passWithNoTests",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/ tests/ --ext .js",
    "lint:fix": "eslint src/ tests/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "seed": "node scripts/seed.js",
    "migrate": "node scripts/migrate.js",
    "backup": "node scripts/backup.js"
  },
  "keywords": ["audio", "platform", "express", "prisma", "postgresql"],
  "author": "qwertifyed",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "aws-sdk": "^2.1505.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "minio": "^7.1.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "redis": "^4.6.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}